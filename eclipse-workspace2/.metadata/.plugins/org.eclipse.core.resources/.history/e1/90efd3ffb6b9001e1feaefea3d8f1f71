package com.simpli.equalsmethod;

public class EqualsDemo {

	public static void main(String[] args) {
		
		String str1 = "Hello";
//		String str2 = "Hello";
		String str2 = new String ("Hello");
		
		if(str1 == str2) {
			System.out.println("both are equal");
		}
		else {
			System.out.println("both are not equal");
		}
	}
}
