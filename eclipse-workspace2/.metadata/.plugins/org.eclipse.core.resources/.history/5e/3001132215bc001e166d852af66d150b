package com.simpli.fileio2;

public class Book {

	private int id;
	private String name;
	private double salary;
	
	public Book() {
		// TODO Auto-generated constructor stub
	}
	
	public Book(int id, String name, double salary) {
		super();
		this.id = id;
		this.name = name;
		this.salary = salary;
	}

	@Override
	public String toString() {
		return "Book [id=" + id + ", name=" + name + ", salary=" + salary + "]";
	}
	
	
	
}
